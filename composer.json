{
  "name": "w3scout/contao-spotify-bundle",
  "description": "Adds a spotify button module",
  "keywords":["contao","spotify","music"],
  "type": "contao-bundle",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Darko Selesi",
      "homepage": "https://github.com/w3scout/spotify",
      "role":"Developer"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "contao/core-bundle": "^4.3"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0"
  },
  "conflict": {
    "contao/manager-plugin": "<2.0 || >=3.0",
    "w3scouts/spotify": "*"
  },
  "autoload": {
    "psr-4": {
      "W3Scout\\SpotifyBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "W3Scout\\SpotifyBundle\\ContaoManager\\Plugin"
  }
}